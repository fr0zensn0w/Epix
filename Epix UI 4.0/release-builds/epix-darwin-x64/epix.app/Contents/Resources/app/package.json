{
  "name": "epix",
  "version": "1.0.0",
  "description": "Epix Photo Gallery",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron . --disable-gpu",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac/BE1.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/windows/BE1.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Epix\""
  },
  "author": "liquidsn0w",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0"
  },
  "dependencies": {
    "exif": "^0.6.0",
    "geolib": "^2.0.22",
    "sql.js": "^0.4.0"
  }
}
